@model List<Provincia>

<h1 style="color:blue">Lista de provincias</h1>

<ul class="list-group">
    @foreach (Provincia p in @Model) 
    { 
        <li class="list-group-item">
            @p.Nombre
            <a asp-controller="Catastro"
               asp-action="Municipios"
               asp-route-nombreprovincia="@p.Nombre" 
               class="btn btn-dark">
                Municipios
            </a>
        </li>
    }
</ul>